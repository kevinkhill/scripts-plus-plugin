{
  "name": "strokesplus-plugin-scriptsplus",
  "main": "src/index.js",
  "browser": "Properties/ScriptsPlus.js",
  "license": "MIT",
  "version": "0.1.0",
  "keywords": [
    "ScriptsPlus",
    "Plugin",
    "Scripts",
    "StrokesPlus",
    "StrokesPlus.net"
  ],
  "author": {
    "name": "Kevin Hill",
    "email": "kevinkhill@gmail.com",
    "website": "http://kevinhill.codes"
  },
  "homepage": "https://github.com/kevinkhill/scripts-plus",
  "scripts": {
    "tasks": "ntl",
    "grunt": "grunt",
    "dev": "webpack --progress --watch",
    "lint": "eslint --ext=.js ./src ./tools",
    "fix": "npm run lint -- --fix",
    "build": "webpack --progress --production",
    "clean": "npm run precompile",
    "reload-sp": "node tools/reload_sp.js",
    "hello": "node tools/say_hello.js",
    "precompile": "dotnet msbuild -nologo -t:Clean ScriptsPlusPlugin.sln",
    "compile": "dotnet msbuild -nologo -p:Configuration=Release ScriptsPlusPlugin.sln",
    "dev-dts": "cd ./tools/DtsGenerator && nodemon DtsGenerator.js",
    "generate-dts": "node ./tools/DtsGenerator/DtsGenerator.js",
    "dev-json": "cd ./tools/JsonGenerator && nodemon JsonGenerator.js",
    "create-json": "npm run generate-json && npx wait 3000 && npm run validate-json",
    "generate-json": "node ./tools/JsonGenerator/JsonGenerator.js",
    "validate-json": "validjson ./tools/JsonGenerator/output.json",
    "format-json": "cd ./tools/JsonGenerator && npx format-jsonfile ./output.json -w 2 -o ./splus-methods.json"
  },
  "ntl": {
    "descriptions": {
      "build": "Runs webpack and then compiles the plugin."
    }
  },
  "dependencies": {
    "json-stringify-safe": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "chokidar": "^3.5.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "execa": "^5.0.0",
    "express": "^4.17.1",
    "format-json-files": "^1.0.6",
    "format-jsonfile": "^1.0.3",
    "grunt": "^1.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^23.0.0",
    "grunt-msbuild": "^1.1.1",
    "grunt-prettier": "^2.1.0",
    "grunt-prompt": "^1.3.3",
    "grunt-shell": "^3.0.1",
    "hook-shell-script-webpack-plugin": "^0.1.3",
    "is-json": "^2.0.1",
    "load-grunt-tasks": "^5.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "ntl": "^5.1.0",
    "prettier": "^2.2.1",
    "valid-json-cli": "^1.4.1",
    "wait": "^0.4.2",
    "wait-on": "^5.2.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-shell-plugin-next": "^2.1.1"
  }
}
