{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "globals": {
    "sp": "readonly",
    "clr": "readonly",
    "forms": "readonly",
    "File": "readonly",
    "Font": "readonly",
    "FontStyle": "readonly",
    "WebBrowser": "readonly",
    "Directory": "readonly"
  },
  "plugins": [
    "prettier"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": "error"
  }
}
